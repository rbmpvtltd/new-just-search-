{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "remoteCache": {
    "signature": true
  },
  "globalEnv": [
    "CLOUDINARY_CLOUD_NAME",
    "CLOUDINARY_API_KEY",
    "CLOUDINARY_API_SECRET",
    "CLOUDINARY_URL",
    "CLOUDINARY_CLOUD_NAME",
    "CLOUDINARY_API_KEY",
    "CLOUDINARY_API_SECRET",
    "CLOUDINARY_URL",
    "NEXT_PUBLIC_ALGOLIA_APP_ID",
    "ALGOLIA_ADMIN_API_KEY",
    "PGHOST",
    "PGUSER",
    "PGPASSWORD",
    "PGPORT",
    "PGDATABASE",
    "BCRYPT_SALT",
    "MYSQL_HOST",
    "MYSQL_USER",
    "MYSQL_PASSWORD",
    "MYSQL_DATABASE"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "start": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "setup": {
      "cache": false,
      "persistent": false,
      "dependsOn": ["^setup"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "seeds": {
      "cache": false,
      "persistent": false
    },
    "dbsync": {
      "cache": false,
      "persistent": false
    }
  }
}
